@model Survey
@{
    ViewData["Result"] = "Result Page";
}

<div class="w-25 regContainer h-100">
    <h1 class="text-decoration-underline name">@Model.Name</h1>
    <div class="border border border-dark border-1 rounded p-3">
        <p> Starting Location: @Model.Location</p>
        <p> Spoken Language: @Model.Language</p>
        <p> Lore(optional): @Model.Comment</p>
        <a href="/" class="btn btn-warning">Go Back</a>
    </div>
</div>